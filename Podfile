# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'TracTracker' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TracTracker
	pod ‘ABGaugeViewKit’
	pod 'LMGaugeViewSwift'
	pod 'MeterGauge'

  target 'TracTrackerTests' do
    inherit! :search_paths
    # Pods for testing
	pod ‘ABGaugeViewKit’
	pod 'LMGaugeViewSwift'
	pod 'MeterGauge'
  end

  target 'TracTrackerUITests' do
    # Pods for testing
	pod ‘ABGaugeViewKit’
	pod 'LMGaugeViewSwift'
	pod 'MeterGauge'
  end

end
